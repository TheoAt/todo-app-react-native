{
  "expo": {
    "name": "TouDou",
    "description": "Une application type ToDo liste développée en React Native.",
    "slug": "todo-app-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_toudou.png",
    "splash": {
      "image": "./assets/splash_toudou.png",
      "resizeMode": "cover",
      "backgroundColor": "#0d1117"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptative_icon_toudou.png",
        "backgroundColor": "#0d1117"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
